{
  "expo": {
    "name": "Appart",
    "slug": "Appart",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/App-Icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.capstone.appart"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/App-Icon.png"
      },
      "package": "com.capstone.appart",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Splash-Screen.png"
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMaps": {
            "accessToken": "pk.eyJ1IjoiaGFpaWFuZXhlIiwiYSI6ImNtMmloZm0zdzBsdDEya3EydHhweHpxazAifQ.T4jDThXZWpOi2GnDdUOzzQ"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow App to access your location for mapping and navigation."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you share images",
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7b61ff4-092d-4a8a-b217-89680e92faa4"
      }
    }
  }
}
